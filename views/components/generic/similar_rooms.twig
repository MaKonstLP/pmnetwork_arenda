{% if items|length > 0 %}

    <div class="listing_widget_header">
        <h3 class="listing_widget_title">Похожие залы</h3>
    </div>

    <div class="items_list swiper-wrapper" data-listing-list data-similar-listing-wrapper>
        {{ this.render('//components/generic/listing.twig', {'items':items, 'type':'similar'}) | raw }}
    </div>

{% endif %}
